amends "AppConfig.pkl"

customObjectMap = new Mapping {
  ["key"] = "valor"
}

secret = read?("env:PORT")?.toString() ?? "not found"

port = 8080

api {
  auth {
    uri = if (!secret.isEmpty) "localhost:\(port.toString())" else 9090.toString()
  }
}
user {
  roles = List("value1","value2")
}

featureToggleEnabled = true;

server {
  port = 9090
  endpoints {
    new {
      name = "endpoint1"
      port = 1234
    }
    new {
      name = "endpoint2"
      port = 5678
    }
  }
}